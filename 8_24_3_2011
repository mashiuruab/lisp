(define (f x y)
  (+ (* 3 x) (* y y)))
(define (my-divide n)
  (cond
    [(= n 0) 'inf]
    [else (/ 1 n)]))

(define-struct point(x y z))
(define-struct personnel-record (name salary dob ssn))
(define-struct child (parents dob date))
(define-struct ball (x y speed-x speed-y))